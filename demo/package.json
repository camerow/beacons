{
  "name": "beacons",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "2json": "node -r esm ./scripts/scss2json.js",
    "names": "node -r esm ./scripts/getBeaconNames.js",
    "0names": "node -r esm ./scripts/getUnnamedBeacons.js",
    "+names": "node -r esm ./scripts/addHardcodedNames.js",
    "clear": "node -r esm ./scripts/clear.js",
    "step1": "node -r esm ./scripts/scss2json.js && node -r esm ./scripts/getBeaconNames.js && node -r esm ./scripts/getUnnamedBeacons.js",
    "step2": "node -r esm ./scripts/addHardcodedNames.js && vue-cli-service build"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/eslint-config-standard": "^5.1.2",
    "axios": "^0.20.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^6.2.2",
    "esm": "^3.2.25",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "vue-template-compiler": "^2.6.11"
  }
}
